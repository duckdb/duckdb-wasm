{
    "name": "@duckdb/duckdb-wasm",
    "version": "0.0.28",
    "description": "DuckDB powered by WebAssembly",
    "license": "MIT",
    "repository": "git://github.com/duckdb/duckdb-wasm.git",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "dependencies": {
        "apache-arrow": "^4.0.0",
        "wasm-feature-detect": "^1.2.11"
    },
    "devDependencies": {
        "@types/emscripten": "^1.39.4",
        "@types/jasmine": "^3.7.6",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "esbuild": "^0.12.9",
        "eslint": "^7.27.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jasmine": "^4.1.2",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "jasmine": "3.7.0",
        "jasmine-spec-reporter": "^7.0.0",
        "karma": "^6.3.4",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.0.3",
        "karma-firefox-launcher": "^2.1.1",
        "karma-jasmine": "^4.0.1",
        "karma-jasmine-html-reporter": "^1.6.0",
        "karma-sourcemap-loader": "^0.3.8",
        "karma-spec-reporter": "^0.0.32",
        "make-dir": "^3.1.0",
        "nyc": "^15.1.0",
        "prettier": "^2.3.0",
        "puppeteer": "^10.0.0",
        "rimraf": "^3.0.2",
        "typedoc": "^0.20.36",
        "typescript": "^4.3.3",
        "web-worker": "^1.0.0"
    },
    "optionalDependencies": {
        "fsevents": "*"
    },
    "scripts": {
        "build:debug": "node bundle.mjs debug && tsc --emitDeclarationOnly",
        "build:release": "node bundle.mjs release && tsc --emitDeclarationOnly",
        "docs": "typedoc --out docs --excludeProtected --includeVersion src/index.ts",
        "report": "node ./coverage.mjs",
        "test:node": "node --experimental-wasm-eh ../../node_modules/jasmine/bin/jasmine ./dist/tests-node.js",
        "test:node:filter": "node --experimental-wasm-eh ../../node_modules/jasmine/bin/jasmine ./dist/tests-node.js --filter=\"File access Large Parquet file\"",
        "test:node:cov": "nyc -r json --report-dir ./coverage/node node --experimental-wasm-eh ../../node_modules/jasmine/bin/jasmine ./dist/tests-node.js",
        "test:browser": "karma start karma.conf.cjs",
        "test:browser:cov": "karma start karma.conf.cov.cjs",
        "test:browser:dbg": "karma start karma.conf.dbg.cjs",
        "test": "npm run test:browser && npm run test:node",
        "test:cov": "npm run test:browser:cov && npm run test:node:cov && npm run report",
        "test:ci": "npm run test:cov",
        "lint": "yarn run eslint src test types"
    },
    "files": [
        "dist/*.d.ts",
        "dist/types/**/*.d.ts",
        "dist/chunk*.js",
        "dist/duckdb*.js",
        "dist/duckdb.wasm"
    ],
    "main": "./dist/duckdb-node.js",
    "types": "./dist/duckdb-node.d.ts",
    "module": "./dist/duckdb.module.js",
    "browser": {
        "./dist/duckdb-node.js": "./dist/duckdb-browser.js",
        "temp-write": false,
        "worker_threads": false,
        "perf_hooks": false,
        "fs": false,
        "os": false,
        "path": false,
        "buffer": false,
        "crypto": false
    }
}

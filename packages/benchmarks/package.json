{
    "name": "@duckdb/benchmarks",
    "version": "0.0.0",
    "license": "MIT",
    "private": true,
    "dependencies": {
        "@duckdb/duckdb-wasm": "^0.0.39",
        "@nano-sql/core": "^2.3.7",
        "alasql": "^1.7.2",
        "apache-arrow": "^4.0.0",
        "arquero": "^4.3.0",
        "kleur": "^4.1.4",
        "lodash": "^4.17.21",
        "lovefield-ts": "^0.7.0",
        "sql.js": "^1.6.2",
        "web-worker": "^1.0.0"
    },
    "devDependencies": {
        "buffalo-bench": "https://github.com/ankoh/buffalo-bench#0a1ee2b534973044a30092fc4a01c443c66b9ecd",
        "@types/faker": "^5.5.8",
        "@types/lodash": "^4.14.175",
        "@types/sql.js": "^1.4.2",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "assert": "^2.0.0",
        "eslint": "^7.31.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.1",
        "faker": "^5.5.3",
        "karma": "^6.3.3",
        "karma-custom": "^1.1.9",
        "karma-webpack": "^5.0.0",
        "prettier": "^2.4.1",
        "ts-loader": "^9.1.0",
        "webpack": "^5.57.1",
        "webpack-inject-plugin": "^1.5.5"
    },
    "scripts": {
        "build:node": "node ./bundle.mjs & tsc --noEmit",
        "build:browser": "webpack --config webpack.config.mjs",
        "build": "yarn run build:node && yarn run build:browser",
        "lint": "yarn run eslint src types",
        "bench:browser": "node ./node_modules/.bin/karma start karma.conf.cjs",
        "bench:node:internal": "node --enable-source-maps --experimental-wasm-eh --experimental-wasm-simd --max-old-space-size=4096 ./dist/node/bench-internal-node.js",
        "bench:node:system": "node --enable-source-maps --experimental-wasm-eh --experimental-wasm-simd --max-old-space-size=4096 ./dist/node/bench-system-node.js"
    }
}
